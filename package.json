{
  "name": "cron-adapter",
  "display_name": "Cron",
  "version": "0.2.1",
  "description": "Allows you to use cron expressions as a trigger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/cron-adapter.git"
  },
